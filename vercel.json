{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install --include=dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/v1/search-rankings/cron?action=create",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/v1/search-rankings/cron?action=process",
      "schedule": "0 14 * * *"
    }
  ]
}
