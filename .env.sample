# API URLs
# Development URLs
NEXT_PUBLIC_CHAT_API_URL=http://localhost:3000    # For chat endpoints (Next.js server)
NEXT_PUBLIC_API_URL=http://localhost:8080         # For other endpoints (Backend server)

# Production URLs (uncomment and update for production)
# NEXT_PUBLIC_CHAT_API_URL=https://satxbounce.com  # For chat endpoints
# NEXT_PUBLIC_API_URL=https://paparaz.me           # For other endpoints

# Analytics
NEXT_GOOGLE_ANALYTICS_ID=your_string_here
NEXT_GTM_ID=your_string_here

# Cloudinary
NEXT_CLOUDINARY_CLOUD_NAME=your_string_here
NEXT_CLOUDINARY_API_KEY=your_string_here
NEXT_CLOUDINARY_API_SECRET=your_string_here
NEXT_CLOUDINARY_API_CLOUDINARY_URL=your_string_here

# MongoDB Chat Database
MONGODB_URI=your_mongodb_uri_here

# Authentication
# Must match the JWT_SECRET used in the backend API for token verification
JWT_SECRET=your_jwt_secret_here
