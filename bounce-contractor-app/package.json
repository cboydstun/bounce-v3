{
  "name": "partypad",
  "private": true,
  "version": "1.0.0-alpha.2",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "dev:prod:host": "vite --mode production --host 0.0.0.0",
    "dev:mobile": "vite --mode mobile --host 0.0.0.0",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "build:analyze": "npm run build && npm run analyze",
    "analyze": "node scripts/analyze-bundle.js",
    "preview": "vite preview",
    "preview:host": "vite preview --host 0.0.0.0",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "cap:sync": "npx cap sync",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "cap:build:android": "npm run build:prod && npx cap sync android && npx cap build android",
    "cap:build:ios": "npm run build:prod && npx cap sync ios && npx cap build ios",
    "cap:clean:android": "rm -rf dist/ && rm -rf android/app/src/main/assets/public/ && npm run build:prod && npx cap sync android && npx cap run android",
    "cap:clean:ios": "rm -rf dist/ && rm -rf ios/App/App/public/ && npm run build:prod && npx cap sync ios && npx cap run ios",
    "build:alpha": "./scripts/build-alpha.sh",
    "build:firebase": "./scripts/build-firebase-alpha.sh",
    "build:ios-dev": "./scripts/build-ios-dev.sh",
    "build:firebase-ios": "./scripts/build-firebase-ios.sh"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "^7.3.0",
    "@capacitor/geolocation": "^7.1.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "^7.3.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/local-notifications": "^7.0.2",
    "@capacitor/network": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@capgo/capacitor-native-biometric": "^7.1.7",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@ionic/storage": "^4.0.0",
    "@tanstack/react-query": "^5.77.2",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "capacitor-secure-storage-plugin": "^0.11.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.8.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "ionicons": "^7.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.5.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^22.15.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.17.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "tailwindcss": "^4.1.8",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.24.0",
    "vite": "~5.2.0",
    "vitest": "^0.34.6"
  },
  "description": "PartyPad - The ultimate mobile app for bounce house contractors to discover, claim, and manage party setup tasks"
}
